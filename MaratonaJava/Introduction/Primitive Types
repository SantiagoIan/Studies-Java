public class Aula01TiposPrimitivos {
    public static void main(String[] args) {
        byte num = 127;
        short num1 = 10;
        int num2 = 12; // geralmente mais usado para numeros inteiros
        long num3 = 8;

        float num4 = 9.5F; // (F) Usado para for√ßar um float sendo double
        double num5 = 7.5; // Geralmente mais usado para numeros decimais

        boolean verdadeiro = true;
        boolean falso = false;

        char caractere = 'A';
    }
}
